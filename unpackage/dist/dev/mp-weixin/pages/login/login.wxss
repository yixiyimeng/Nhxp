page,
.pageview {
  height: 100%;
  width: 100%;
}
.group {
  margin: 0 30rpx;
  font-size: 26rpx;
}
.group .row {
    border-bottom: 1px solid #e5e5e5;
    margin: 0 15rpx;
    padding: 25rpx 20rpx;
}
.group .row .getcode {
      color: #fff;
      width: 165rpx;
      height: 60rpx;
      line-height: 60rpx;
      text-align: center;
      -webkit-border-radius: 20rpx;
              border-radius: 20rpx;
      background-image: -webkit-gradient(linear, left top, right top, from(#fa9649), to(#fc713a));
      background-image: -webkit-linear-gradient(left, #fa9649, #fc713a);
      background-image: -o-linear-gradient(left, #fa9649, #fc713a);
      background-image: linear-gradient(90deg, #fa9649, #fc713a);
}
.otherway {
  text-align: center;
  font-size: 26rpx;
  color: #878787;
}
.otherway .title {
    margin: 0 30rpx;
    font-weight: bold;
}
.otherway .title .line {
      height: 1px;
      background: #e5e5e5;
}
.otherway .title > view:nth-child(2) {
      margin: 0 30rpx;
}
.otherway image {
    display: block;
    margin: 35rpx auto 25rpx;
    width: 116rpx;
}
.otherway navigator {
    display: inline-block;
}


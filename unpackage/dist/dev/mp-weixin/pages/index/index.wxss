.card {
  -webkit-border-radius: 30rpx;
          border-radius: 30rpx;
  position: relative;
  overflow: hidden;
  padding: 35rpx 20rpx;
  background: #fff;
  colocolor: #000;
}
.card .tjtag {
    position: absolute;
    left: 30rpx;
    top: 0;
    height: 58rpx;
    width: 58rpx;
    -webkit-border-radius: 0 20rpx 0 20rpx;
            border-radius: 0 20rpx 0 20rpx;
    color: #fff;
    font-size: 16rpx;
    padding: 10rpx;
    background: #e60012;
    z-index: 99;
    text-align: center;
}
.card .tjtag.tjtag1 {
      background: #917bdd;
}
.card .name {
    font-size: 30rpx;
    font-weight: bold;
}
.card .imgbox {
    width: 191rpx;
    height: 191rpx;
    margin-right: 35rpx;
}
.card .imgbox image {
      height: 100%;
      width: 100%;
      display: block;
}
.card .num {
    color: #b3b6be;
    font-size: 24rpx;
    padding: 20rpx 0 30rpx;
    border-bottom: 1px solid #f2f3f7;
}
.card .price {
    color: #e03d3e;
    font-size: 38rpx;
    font-weight: bold;
}
.card .shopinfo {
    font-size: 20rpx;
    color: #666;
    margin-top: 10rpx;
    position: relative;
}
.card .shopinfo .link {
      color: #000;
      margin-left: 12rpx;
}
.card .shopinfo .tag {
      color: #fff;
      background: #e60012;
      line-height: 25rpx;
      height: 25rpx;
      display: inline-block;
      padding: 0 10rpx;
      -webkit-border-radius: 5rpx;
              border-radius: 5rpx;
      font-size: 16rpx;
      margin-right: 10rpx;
}
.card .shopinfo .favstate {
      color: #fb8442;
}
.card .shopinfo .btn {
      position: absolute;
      bottom: 0;
      right: 0;
      color: #fff;
      font-size: 24rpx;
      width: 150rpx;
      line-height: 60rpx;
      -webkit-border-radius: 15rpx;
              border-radius: 15rpx;
      text-align: center;
      background-image: -webkit-gradient(linear, left top, right top, from(#fe8c2d), to(#fe5504));
      background-image: -webkit-linear-gradient(left, #fe8c2d, #fe5504);
      background-image: -o-linear-gradient(left, #fe8c2d, #fe5504);
      background-image: linear-gradient(90deg, #fe8c2d, #fe5504);
}
.card .info {
    background: #f2f3f7;
    margin-top: 30rpx;
    padding: 5rpx 20rpx;
}
.card .info > view {
      width: 30%;
      padding: 10rpx 0;
}
.card .info > view:nth-child(3n+2) {
        width: 40%;
}
.card .fav,
  .card .share {
    height: 40rpx;
    width: 40rpx;
    background: no-repeat center center;
    background-size: contain;
    display: inline-block;
}
.card .fav {
    background-image: url(data:image/png;base64,);
    margin-right: 30rpx;
}
.card .share {
    background-image: url(data:image/png;base64,);
}

page,
.view-page {
  height: 100%;
  position: relative;
  background: #f0f0f0;
}
.tag {
  color: #7f7f7f;
  font-size: 22rpx;
}
.tag ._span + ._span {
    margin-left: 25rpx;
}
.tag .icon {
    display: inline-block;
    margin-right: 10rpx;
    height: 24rpx;
    width: 24rpx;
    vertical-align: middle;
    background: url(data:image/png;base64,) no-repeat center center;
}
.tag .icon.icon1 {
      background-size: 24rpx auto;
}
.tag .icon.icon2 {
      background-image: url(data:image/png;base64,);
      background-size: 19rpx auto;
}
.tag .icon.icon3 {
      background-image: url(data:image/png;base64,);
      background-size: 20rpx auto;
}
.tag .icon.icon4 {
      background-image: url(data:image/png;base64,);
      background-size: 18rpx auto;
}
.tag .icon + text {
    vertical-align: middle;
}
.grid {
  margin: 30rpx auto 10rpx;
}
.grid .imgbox {
    height: 94rpx;
    width: 94rpx;
    margin: 0 auto 20rpx;
}
.grid .imgbox image {
      height: 94rpx;
      width: 94rpx;
      display: block;
}
.grid .imgbox + text {
    font-size: 22rpx;
    color: #000;
    padding-bottom: 16rpx;
    margin-top: 0;
}
.notice {
  background-image: -webkit-linear-gradient(45deg, #fe8c2d, #fe5504);
  background-image: -o-linear-gradient(45deg, #fe8c2d, #fe5504);
  background-image: linear-gradient(45deg, #fe8c2d, #fe5504);
  color: #fff;
  height: 70rpx;
  font-size: 20rpx;
}
.notice .title {
    font-size: 30rpx;
    padding-left: 80rpx;
}
.notice .line {
    height: 25rpx;
    width: 2rpx;
    background: #fff;
    margin: 0 18rpx;
}
.tabnav {
  text-align: center;
  margin: 30rpx 0;
}
.tabnav .line {
    width: 2rpx;
    height: 60rpx;
    background: #d9d9d9;
}
.tabnav .tab-item .title {
    color: #000;
    font-weight: blod;
    font-size: 28rpx;
}
.tabnav .tab-item .tag {
    font-size: 22rpx;
    color: #848484;
    width: 120rpx;
    height: 35rpx;
    line-height: 35rpx;
    -webkit-border-radius: 100rpx;
            border-radius: 100rpx;
    margin: 10rpx auto 0;
}
.tabnav .tab-item.active .tag {
    background-image: -webkit-linear-gradient(45deg, #fe8c2d, #fe5504);
    background-image: -o-linear-gradient(45deg, #fe8c2d, #fe5504);
    background-image: linear-gradient(45deg, #fe8c2d, #fe5504);
    color: #fff;
}
.tabnav .tab-item.active .title {
    color: #f65900;
}
.list {
  margin: 0 25rpx;
}
.list > goods {
    margin-bottom: 30rpx;
}


page,
.view-page {
  height: 100%;
  position: relative;
  background: #f0f0f0;
}
.tag {
  color: #7f7f7f;
  font-size: 22rpx;
}
.tag ._span + ._span {
    margin-left: 25rpx;
}
.tag .icon {
    display: inline-block;
    margin-right: 10rpx;
    height: 24rpx;
    width: 24rpx;
    vertical-align: middle;
    background: url(data:image/png;base64,) no-repeat center center;
}
.tag .icon.icon1 {
      background-size: 24rpx auto;
}
.tag .icon.icon2 {
      background-image: url(data:image/png;base64,);
      background-size: 19rpx auto;
}
.tag .icon.icon3 {
      background-image: url(data:image/png;base64,);
      background-size: 20rpx auto;
}
.tag .icon.icon4 {
      background-image: url(data:image/png;base64,);
      background-size: 18rpx auto;
}
.tag .icon + text {
    vertical-align: middle;
}
.grid {
  margin: 30rpx auto 10rpx;
}
.grid .imgbox {
    height: 94rpx;
    width: 94rpx;
    margin: 0 auto 20rpx;
}
.grid .imgbox image {
      height: 94rpx;
      width: 94rpx;
      display: block;
}
.grid .imgbox + text {
    font-size: 22rpx;
    color: #000;
    padding-bottom: 16rpx;
    margin-top: 0;
}
.notice {
  background-image: -o-linear-gradient(45deg, #fe8c2d, #fe5504);
  background-image: linear-gradient(45deg, #fe8c2d, #fe5504);
  color: #fff;
  height: 70rpx;
  font-size: 20rpx;
}
.notice .title {
    font-size: 30rpx;
    padding-left: 80rpx;
}
.notice .line {
    height: 25rpx;
    width: 2rpx;
    background: #fff;
    margin: 0 18rpx;
}
.tabnav {
  text-align: center;
  margin: 30rpx 0;
}
.tabnav .line {
    width: 2rpx;
    height: 60rpx;
    background: #d9d9d9;
}
.tabnav .tab-item .title {
    color: #000;
    font-weight: blod;
    font-size: 28rpx;
}
.tabnav .tab-item .tag {
    font-size: 22rpx;
    color: #848484;
    width: 120rpx;
    height: 35rpx;
    line-height: 35rpx;
    border-radius: 100rpx;
    margin: 10rpx auto 0;
}
.tabnav .tab-item.active .tag {
    background-image: -o-linear-gradient(45deg, #fe8c2d, #fe5504);
    background-image: linear-gradient(45deg, #fe8c2d, #fe5504);
    color: #fff;
}
.tabnav .tab-item.active .title {
    color: #f65900;
}
.list {
  margin: 0 25rpx;
}
.list > goods {
    margin-bottom: 30rpx;
}


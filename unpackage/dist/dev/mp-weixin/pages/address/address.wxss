page,
.pageview {
  height: 100%;
  width: 100%;
  background: #f0f0f0;
  overflow: hidden;
}
page .list,
  .pageview .list {
    overflow: auto;
    padding: 30rpx 0;
}
page .addbtn,
  .pageview .addbtn {
    font-size: 26rpx;
    color: #000;
    line-height: 120rpx;
    text-align: center;
    border-top: 1px solid #c9c9c9;
}


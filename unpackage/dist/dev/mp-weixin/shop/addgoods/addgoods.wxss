page {
  height: 100%;
  background: #fff;
}
.tip {
  padding: 20rpx 30rpx;
  color: #545454;
  font-size: 26rpx;
  line-height: 38rpx;
  background: #F7F7F7;
}
.tip .cuIcon {
    color: #19A967;
    font-size: 48rpx;
    margin-right: 10rpx;
}
.cu-form-group input {
  text-align: right;
}
.cu-form-group radio {
  -webkit-transform: scale(0.7);
      -ms-transform: scale(0.7);
          transform: scale(0.7);
  vertical-align: 3px;
}
.uploadCard {
  background: #fff;
  padding: 38rpx 30rpx;
}
.uploadCard .upload {
  width: 324rpx;
  height: 279rpx;
}
.sendBtn {
  width: 690rpx;
  height: 100rpx;
  background: #19A967;
  -webkit-box-shadow: 0 9rpx 27rpx 0 rgba(25, 169, 103, 0.58);
          box-shadow: 0 9rpx 27rpx 0 rgba(25, 169, 103, 0.58);
  border-radius: 12rpx;
  color: #fff;
  text-align: center;
  font-size: 30rpx;
  line-height: 100rpx;
  margin: 30rpx auto;
}
.txt {
  padding: 10rpx 0 40rpx;
  font-size: 26rpx;
  text-align: center;
  color: #6F6F6F;
}
.txt navigator {
    color: #19A967;
    display: inline;
}


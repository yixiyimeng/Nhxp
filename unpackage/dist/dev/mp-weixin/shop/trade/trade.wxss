.filterbar {
  padding: 35rpx 30rpx 50rpx;
}
.filterbar .title {
    color: #2F2F2F;
    font-size: 30rpx;
}
.filterbar .inputbox {
    height: 70rpx;
    line-height: 70rpx;
    border: 1px solid #9D9D9D;
    -webkit-border-radius: 8rpx;
            border-radius: 8rpx;
    padding: 0 18rpx;
}
.filterbar .inputbox image {
      width: 28rpx;
}
.filterbar .line {
    width: 18rpx;
    background: #E3E3E3;
    height: 3rpx;
    margin: 0 10rpx;
}
.filterbar .searchBtn {
    width: 600rpx;
    height: 100rpx;
    background: #19a967;
    -webkit-box-shadow: 0px 9rpx 27rpx 0px rgba(25, 169, 103, 0.58);
            box-shadow: 0px 9rpx 27rpx 0px rgba(25, 169, 103, 0.58);
    -webkit-border-radius: 12rpx;
            border-radius: 12rpx;
    color: #fff;
    color: #fff;
    font-size: 30rpx;
    text-align: center;
    line-height: 100rpx;
    margin: 46rpx auto 0;
}
.space {
  height: 14rpx;
  background: #F5F5F5;
}
.tablebox .title {
  color: #2E2E2E;
  font-size: 28rpx;
  font-weight: bold;
}
.tablebox ._p {
  margin-top: 30rpx;
}


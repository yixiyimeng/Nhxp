.pageview,
page {
  height: 100%;
  width: 100%;
}
.navbar {
  height: 100rpx;
  color: #626262;
  padding: 0 50rpx;
  border-bottom: 1px solid #EAEAEA;
  font-size: 30rpx;
}
.navbar .active {
    color: #19a967;
    position: relative;
}
.navbar .active:after {
      width: 35rpx;
      height: 5rpx;
      background: #19a967;
      border-radius: 3rpx;
      position: absolute;
      bottom: 0;
      left: 50%;
      -webkit-transform: translateX(-50%);
          -ms-transform: translateX(-50%);
              transform: translateX(-50%);
}
.list {
  overflow: auto;
  padding-top: 20rpx;
}


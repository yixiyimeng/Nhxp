.card {
  border-radius: 30rpx;
  position: relative;
  overflow: hidden;
  padding: 35rpx 20rpx;
  background: #fff;
  colocolor: #000;
}
.card .tjtag {
    position: absolute;
    left: 30rpx;
    top: 0;
    height: 58rpx;
    width: 58rpx;
    border-radius: 0 20rpx 0 20rpx;
    color: #fff;
    font-size: 16rpx;
    padding: 10rpx;
    background: #e60012;
    z-index: 99;
    text-align: center;
}
.card .tjtag.tjtag1 {
      background: #917bdd;
}
.card .name {
    font-size: 30rpx;
    font-weight: bold;
}
.card .imgbox {
    width: 191rpx;
    height: 191rpx;
    margin-right: 35rpx;
}
.card .imgbox image {
      height: 100%;
      width: 100%;
      display: block;
}
.card .num {
    color: #b3b6be;
    font-size: 24rpx;
    padding: 20rpx 0 30rpx;
    border-bottom: 1px solid #f2f3f7;
}
.card .price {
    color: #e03d3e;
    font-size: 38rpx;
    font-weight: bold;
}
.card .shopinfo {
    font-size: 20rpx;
    color: #666;
    margin-top: 10rpx;
    position: relative;
}
.card .shopinfo .link {
      color: #000;
      margin-left: 12rpx;
}
.card .shopinfo .tag {
      color: #fff;
      background: #e60012;
      line-height: 25rpx;
      height: 25rpx;
      display: inline-block;
      padding: 0 10rpx;
      border-radius: 5rpx;
      font-size: 16rpx;
      margin-right: 10rpx;
}
.card .shopinfo .favstate {
      color: #fb8442;
}
.card .shopinfo .btn {
      position: absolute;
      bottom: 0;
      right: 0;
      color: #fff;
      font-size: 24rpx;
      width: 150rpx;
      line-height: 60rpx;
      border-radius: 15rpx;
      text-align: center;
      background-image: -webkit-gradient(linear, left top, right top, from(#fe8c2d), to(#fe5504));
      background-image: -o-linear-gradient(left, #fe8c2d, #fe5504);
      background-image: linear-gradient(90deg, #fe8c2d, #fe5504);
}
.card .info {
    background: #f2f3f7;
    margin-top: 30rpx;
    padding: 5rpx 20rpx;
}
.card .info > view {
      width: 30%;
      padding: 10rpx 0;
}
.card .info > view:nth-child(3n+2) {
        width: 40%;
}
.card .fav,
  .card .share {
    height: 40rpx;
    width: 40rpx;
    background: no-repeat center center;
    background-size: contain;
    display: inline-block;
}
.card .fav {
    background-image: url(data:image/png;base64,);
    margin-right: 30rpx;
}
.card .share {
    background-image: url(data:image/png;base64,);
}


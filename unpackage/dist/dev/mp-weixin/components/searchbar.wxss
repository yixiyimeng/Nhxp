.searchbar {
  padding: 14rpx 30rpx;
  border: 1px solid #F3F3F3;
}
.searchbar image {
    width: 100%;
    height: 100%;
    display: block;
}
.searchbar > .flex-sub {
    height: 64rpx;
    line-height: 64rpx;
    font-size: 26rpx;
    background: #F7F7F7;
    border-radius: 32rpx;
}
.searchbar > .flex-sub .icon {
      margin: 0 23rpx;
      width: 31rpx;
      height: 31rpx;
}
.searchbar > .flex-sub input {
      font-size: 26rpx;
}
.searchbar .contact {
    margin-left: 29rpx;
    height: 44rpx;
    width: 44rpx;
    display: block;
    padding: 0;
    background: none;
}


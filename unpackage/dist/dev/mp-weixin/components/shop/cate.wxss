.cate-item {
  background: #fff;
  padding: 0 14rpx;
  -webkit-box-shadow: 0 4rpx 16rpx 0 #d5d5d5;
          box-shadow: 0 4rpx 16rpx 0 #d5d5d5;
  border-radius: 8rpx;
  color: #666;
  font-size: 24rpx;
}
.cate-item .title {
    color: #A0A0A0;
    font-size: 28rpx;
    padding: 20rpx 0;
}
.cate-item .infobox {
    padding-bottom: 22rpx;
    border-bottom: 1px solid #EDEDED;
}
.cate-item .infobox .info view {
      width: 30%;
      margin: 10rpx 0;
}
.cate-item .infobox .info view:nth-child(3n+2) {
        width: 40%;
}
.cate-item .imgbox {
    width: 160rpx;
    height: 160rpx;
    border-radius: 8rpx;
    overflow: hidden;
    margin-right: 18rpx;
}
.cate-item .imgbox image {
      width: 100%;
}
.cate-item .btnbar {
    font-size: 24rpx;
    padding: 18rpx 0;
    color: #666;
}
.cate-item .btnbar .price {
      color: #FD3C3E;
      font-size: 22rpx;
}
.cate-item .btnbar .price + .price {
        font-size: 36rpx;
}
.cate-item .btnbar navigator {
      display: inline-block;
      color: #19A967;
      font-size: 26rpx;
      margin-left: 10rpx;
}
.cate-item .btnbar .btn {
    font-size: 26rpx;
    width: 134rpx;
    height: 59rpx;
    text-align: center;
    line-height: 59rpx;
    border-radius: 8rpx;
    color: #FD3C3E;
    background: #fff;
    border: 1px solid #FD3C3E;
}
.cate-item .btnbar .btn + .btn {
      background: #19A967;
      color: #fff;
      margin-left: 22rpx;
      border-color: #19A967;
}
.cate-item .name {
    color: #2E2E2E;
    font-size: 32rpx;
}
.cate-item .state {
    font-size: 26rpx;
    margin-right: 25rpx;
    line-height: 32rpx;
    vertical-align: middle;
}


.evaluate-item {
  margin: 20rpx 30rpx 0;
  background: #fff;
  -webkit-box-shadow: 0px 4rpx 16rpx 0px #d5d5d5;
          box-shadow: 0px 4rpx 16rpx 0px #d5d5d5;
  border-radius: 8rpx;
  padding: 34rpx 24rpx;
  color: #ABABAB;
  font-size: 24rpx;
}
.evaluate-item .title {
    color: #1D1D1D;
    font-size: 30rpx;
}
.evaluate-item .info {
    color: #5F5F5F;
    font-size: 28rpx;
    margin: 25rpx 0;
    line-height: 40rpx;
}

